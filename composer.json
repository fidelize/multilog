{
    "name": "karlomikus/multilog",
    "type": "library",
    "description": "Multiple log files for your laravel application.",
    "keywords": [
        "multilog",
        "logging",
        "log"
    ],
    "homepage": "https://github.com/karlomikus/multilog",
    "license": "MIT",
    "authors": [
        {
            "name": ":Karlo Miku≈°",
            "email": "contact@karlomikus.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php" : "~5.5",
        "monolog/monolog": "~1.11"
    },
    "require-dev": {
        "phpunit/phpunit": "5.*",
        "mockery/mockery": "~0.9.2",
        "illuminate/contracts": "5.2.*",
        "illuminate/support": "5.2.*"
    },
    "autoload": {
        "psr-4": {
            "Karlomikus\\Multilog\\": "src/Multilog"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/"
        ]
    }
}
